{
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer"
            ],
            "settings": {
                "rust-analyzer.cargo.loadOutDirsFromCheck": true,
                "rust-analyzer.cargo.allFeatures": true,
                "rust-analyzer.checkOnSave": true
            }
        }
    }
}
